@model IEnumerable<DoAnCN.Models.Custumer>

@{
    ViewBag.Title = "ListCustumerAd";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>ListCustumerAd</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdCustumer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FullNameCustumer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateOfBirth)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sex)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdTour)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdCustumer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FullNameCustumer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfBirth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sex)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdTour)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditCustumer", new {  id=item.IdCustumer  }) |
            @Html.ActionLink("Delete", "DeleteCustumer", new { id = item.IdCustumer })
        </td>
    </tr>
}

</table>
